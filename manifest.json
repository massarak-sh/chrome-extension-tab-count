{
    "manifest_version": 3,
    "name": "Smart tab count",
    "description": "A Chrome extension devoted to keeping your opened tabs under control",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],
    "options_page": "options.html",
    "action": {
      "default_popup": "ext.html",
      "default_icon": "ext.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  
  }